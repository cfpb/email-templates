{
  "name": "cfpb-email-templates",
  "version": "0.1.0",
  "description": "Templates for CFPB emails, featuring responsive design.",
  "homepage": "http://consumerfinance.gov",
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "email": "tech@cfpb.gov",
    "url": "https://cfpb.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/capital-framework.git"
  },
  "license": "CC0",
  "dependencies": {
    "cf-buttons": "*",
    "cf-icons": "*",
    "cf-typography": "~0.7.0"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "box-sizing-polyfill": {
      "js": "boxsizing.htc"
    },
    "html5shiv": {
      "js": "dist/html5shiv-printshiv.min.js"
    }
  }
}
